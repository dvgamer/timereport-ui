{
  "name": "devops-ui",
  "version": "1.3.0",
  "description": "DevOps project",
  "author": "Kananek T. <info.dvgamer@gmail.com>",
  "private": true,
  "main": "nuxt",
  "scripts": {
    "dev": "env-cmd nuxt dev",
    "serv": "env-cmd nodemon api/index.js --exec \"babel-node\"",
    "test2": "env-cmd node tests/ldap.js",
    "build": "env-cmd api/build.env babel -d ./dist ./api -s && env-cmd api/build.env nuxt build",
    "pack": "7z a devops-ui.zip api/* .nuxt/* dist/* static/* store/* nuxt.config.js package.json",
    "pack2": "7z a node_modules.zip node_modules/*",
    "start:devops": "node dist/main.js --no-deprecation --no-warnings",
    "start:socket": "node api/socket-io",
    "start:auth": "node api/endpoint auth",
    "start:api": "node api/endpoint api",
    "start:log": "node api/endpoint log",
    "start:sync": "env-cmd .env node api/data-sync",
    "eslint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "preinstall": "which link-module-alias && link-module-alias clean || echo",
    "postinstall": "link-module-alias"
  },
  "_moduleAliasIgnoreWarning": true,
  "_moduleAliases": {
    "@debuger": "/api/debuger.js",
    "@mssql": "/api/mssql.js",
    "@auth": "/api/authication/middleware.js",
    "@config": "/api/router/config/get-service-config.js",
    "@global": "/api/router/config/get-global-config.js",
    "@mongo": "/api/mongodb/index.js",
    "@line": "/api/line/index.js",
    "@line-flex": "/api/line/flex/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "consola": "^2.10.1",
    "express": "^4.17.0",
    "jsonwebtoken": "^8.5.0",
    "ldapjs": "^1.0.2",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "mkdir-recursive": "^0.4.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "mongoose": "^5.7.1",
    "mssql": "^5.1.0",
    "node-cron": "^2.0.3",
    "normalize-wheel": "^1.0.1",
    "nuxt": "^2.9.2",
    "promise-ftp": "^1.3.5",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "simple-git": "^1.126.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@nuxtjs/auth": "^4.8.4",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/font-awesome": "^1.0.3",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "babel-eslint": "^10.0.3",
    "backpack-core": "^0.8.3",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.1",
    "chalk": "^2.4.1",
    "codemirror": "^5.48.4",
    "cross-env": "^5.2.1",
    "env-cmd": "^10.0.1",
    "eslint": "^6.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-backpack": "^0.2.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "highcharts": "^7.2.0",
    "link-module-alias": "^1.2.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.2",
    "nuxt-fontawesome": "^0.4.0",
    "sass-loader": "^8.0.0",
    "socket.io-client": "^2.2.0",
    "vue-burger-menu": "^2.0.1",
    "vue-codemirror": "^4.0.6",
    "vue-cookie-law": "^1.9.0",
